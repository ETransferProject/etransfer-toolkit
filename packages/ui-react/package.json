{
  "name": "@etransfer/ui-react",
  "keywords": [
    "antd",
    "etransfer"
  ],
  "description": "",
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist/*"
  ],
  "homepage": "https://etransfer.exchange",
  "repository": {
    "type": "git",
    "url": "https://github.com/ETransferProject/etransfer-toolkit",
    "directory": "packages/ui-react"
  },
  "version": "1.15.0-alpha.0",
  "main": "dist/index.js",
  "types": "dist/_types/index.d.ts",
  "type": "module",
  "scripts": {
    "prebuild": "rm -rf dist",
    "start": "tsc --watch",
    "lint": "eslint . --ext .tsx,.ts",
    "lint:md": "remark . -f -q",
    "build:clean": "rm -rf dist",
    "build:types": "tsc --emitDeclarationOnly",
    "build:source": "rollup --silent --config rollup.config.js",
    "build:svg": "cd ./src/assets && node buildSvg.cjs",
    "build": "npm run build:clean && npm run build:types && npm run build:source",
    "dev": "rollup --config rollup.config.js --watch",
    "release": "yarn version && yarn build",
    "modified:less": "lessc --js --modify-var='ant-prefix=etransfer-ant' src/assets/theme/antd.less src/assets/theme/modified.antd.css",
    "pub": "npm publish",
    "test": "jest"
  },
  "dependencies": {
    "@etransfer/core": "^1.15.0-alpha.0",
    "@etransfer/types": "^1.15.0-alpha.0",
    "@etransfer/utils": "^1.15.0-alpha.0",
    "@portkey/types": "^2.23.0-alpha.7",
    "@portkey/utils": "^2.23.0-alpha.7",
    "antd": "4.24.14",
    "bignumber.js": "^9.1.0",
    "clsx": "^1.2.1",
    "lottie-web": "5.9.6",
    "moment": "^2.29.4",
    "qs": "^6.13.1",
    "query-string": "^7.1.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-qrcode-logo": "2.9.0",
    "react-use": "^17.4.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/uuid": "^8.3.4",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "less": "^4.1.3",
    "postcss": "^8.4.21"
  },
  "peerDependencies": {
    "aelf-sdk": "^3.5.1-beta.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
