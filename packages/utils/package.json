{
  "name": "@etransfer/utils",
  "version": "1.0.0-alpha.4",
  "description": "",
  "main": "./dist/commonjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/commonjs/index.js"
    }
  },
  "author": "",
  "license": "ISC",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "files": [
    "dist/*"
  ],
  "homepage": "https://home.etransfer.exchange/",
  "repository": {
    "type": "git",
    "url": "https://github.com/ETransferProject/etransfer-toolkit.git",
    "directory": "packages/request"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "yarn clean",
    "build": "yarn build:cjs & yarn build:esm & yarn build:types",
    "build:cjs": "tsc && echo '{\"type\": \"commonjs\"}' > ./dist/commonjs/package.json",
    "build:esm": "tsc --build tsconfig.esm.json && echo '{\"type\": \"module\"}' > ./dist/esm/package.json && yarn esm:fix",
    "build:types": "tsc --build tsconfig.types.json",
    "esm:fix": "node ../../scripts/fix-esm.js dist/esm",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "eslint --fix --ext .js,.ts .",
    "format": "prettier --write '**/*'",
    "start": "tsc --watch",
    "test": "jest"
  },
  "dependencies": {
    "@etransfer/services": "^1.0.0-alpha.4",
    "@etransfer/types": "^1.0.0-alpha.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "babel-jest": "^29.3.1",
    "jest": "^29.3.1",
    "ts-jest": "^29.0.5"
  },
  "peerDependencies": {
    "@portkey/contracts": "^2.4.0",
    "@portkey/types": "^2.4.0",
    "@portkey/utils": "^2.4.0",
    "aelf-sdk": "^3.4.7",
    "aelf-web-login": "2.0.1-beta.9",
    "bignumber.js": "^9.1.0",
    "query-string": "^7.1.1"
  }
}
